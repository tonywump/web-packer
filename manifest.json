{
  "name": "Web Packer",
  "version": "1.0",
  "description": "Pack your urls in browser!",
  "permissions": ["tabs","contextMenus"],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action":{
    "default_popup": "popup.html"
  },
  "manifest_version": 3
}
